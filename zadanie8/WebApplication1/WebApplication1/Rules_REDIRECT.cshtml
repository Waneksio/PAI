@{ 
    var rules = (List<string>)Session["Rules"];
    if (rules == null)
    {
        rules = new List<string>();
    }

    if(IsPost)
    {
        rules.Add(Request.Form["new_rule"]);
        Session["Rules"] = rules;
        Response.Redirect("Rules_GET.cshtml");
    }
}

